{
  "variables": {
    "global": {
      "background": "light",
      "font": "iosevka",
      "fonts": {
        "agave": "Agave Nerd Font",
        "cascadia": "CaskaydiaCove NF SemiLight",
        "fantasque": "FantasqueSansM Nerd Font",
        "hack": "Hack Nerd Font",
        "inconsolata": "Inconsolata Nerd Font",
        "iosevka": "Iosevka Nerd Font",
        "jetbrains": "JetBrainsMono Nerd Font",
        "monoid": "Monoid Nerd Font"
      },
      "keyboard": "pro",
      "shortcut": "pro",
      "taskbar": "pro",
      "theme": "catppuccin"
    },
    "applications": {
      "insomnium": {
        "path": "~/.config/Insomnium/insomnia.Settings.db",
        "themes": {
          "dark": {
            "ayu": "ayu-dark",
            "catppuccin": "catppuccin-mocha",
            "onedark": "one-dark"
          },
          "light": {
            "ayu": "ayu-light",
            "catppuccin": "catppuccin-latte",
            "onedark": "one-light"
          }
        }
      },
      "kde": {
        "backgrounds": {
          "dark": "org.kde.breezedark.desktop",
          "light": "org.kde.breeze.desktop"
        },
        "keyboards": {
          "latam": "latam -option",
          "pro": "pro -option caps:swapescape"
        },
        "shortcuts": {
          "default": "kc() { kwriteconfig5 --file \"$1\" --group \"$2\" --key \"$3\" $4; }; kd() { kwriteconfig5 --file \"$1\" --group \"$2\" --key \"$3\" --delete; }; kd \"kdeglobals\" \"KDE\" \"AnimationDurationFactor\"; kd \"kglobalshortcutsrc\" \"google-chrome.desktop\" \"_launch\"; kd \"kglobalshortcutsrc\" \"org.wezfurlong.wezterm.desktop\" \"_launch\"; for i in `seq 9`; do kc \"kglobalshortcutsrc\" \"plasmashell\" \"activate task manager entry $i\" \"Meta+$i,Meta+$i,Activar la entrada $i del gestor de tareas\"; done; kc \"kglobalshortcutsrc\" \"plasmashell\" \"activate task manager entry 10\" \"Meta+0,Meta+0,Activar la entrada 10 del gestor de tareas\"; for i in `seq 5`; do kc \"kglobalshortcutsrc\" \"kwin\" \"Switch to Desktop $i\" \"Ctrl+F$i,Ctrl+F$i,Cambiar al escritorio $i\"; kc \"kglobalshortcutsrc\" \"kwin\" \"Window to Desktop $i\" \"none,none,Ventana al escritorio $i\"; done",
          "pro": "kc() { kwriteconfig5 --file \"$1\" --group \"$2\" --key \"$3\" $4; }; kd() { kwriteconfig5 --file \"$1\" --group \"$2\" --key \"$3\" --delete; }; kc \"kdeglobals\" \"KDE\" \"AnimationDurationFactor\" \"0\"; kc \"kglobalshortcutsrc\" \"google-chrome.desktop\" \"_launch\" \"Meta+B,none,Google Chrome\"; kc \"kglobalshortcutsrc\" \"google-chrome.desktop\" \"_k_friendly_name\" \"Google Chrome\"; kc \"kglobalshortcutsrc\" \"org.wezfurlong.wezterm.desktop\" \"_launch\" \"Meta+T,none,WezTerm\"; kc \"kglobalshortcutsrc\" \"org.wezfurlong.wezterm.desktop\" \"_k_friendly_name\" \"WezTerm\"; for i in `seq 9`; do kc \"kglobalshortcutsrc\" \"plasmashell\" \"activate task manager entry $i\" \"none,Meta+$i,Activar la entrada $i del gestor de tareas\"; done; kc \"kglobalshortcutsrc\" \"plasmashell\" \"activate task manager entry 10\" \"none,Meta+0,Activar la entrada 10 del gestor de tareas\"; for i in `seq 5`; do kc \"kglobalshortcutsrc\" \"kwin\" \"Switch to Desktop $i\" \"Meta+$i,Ctrl+F$i,Cambiar al escritorio $i\"; kc \"kglobalshortcutsrc\" \"kwin\" \"Window to Desktop $i\" \"none,none,Ventana al escritorio $i\"; done; kc \"kglobalshortcutsrc\" \"kwin\" \"Window to Desktop 1\" \"Meta+!,none,Ventana al escritorio 1\";kc \"kglobalshortcutsrc\" \"kwin\" \"Window to Desktop 2\" \"Meta+\\\",none,Ventana al escritorio 2\";kc \"kglobalshortcutsrc\" \"kwin\" \"Window to Desktop 3\" \"Meta+#,none,Ventana al escritorio 3\";kc \"kglobalshortcutsrc\" \"kwin\" \"Window to Desktop 4\" \"Meta+$,none,Ventana al escritorio 4\";kc \"kglobalshortcutsrc\" \"kwin\" \"Window to Desktop 5\" \"Meta+%,none,Ventana al escritorio 5\""
        },
        "slides": {
          "default": "--delete",
          "pro": "false"
        },
        "taskbars": {
          "default": "panels().map(p=\u003ep.remove());desktops().map(d=\u003ed.widgets().map(w=\u003ew.remove()));let p=new Panel;p.location='bottom';p.height=2*Math.floor(gridUnit*2.3/2);let m=p.addWidget('org.kde.plasma.kickoff');m.currentConfigGroup=['Shortcuts'];m.writeConfig('global','Alt+F1');p.addWidget('org.kde.plasma.pager');let t=p.addWidget('org.kde.plasma.icontasks');t.currentConfigGroup=['General'];t.writeConfig('launchers',['applications:org.wezfurlong.wezterm.desktop','applications:google-chrome.desktop','applications:org.kde.dolphin.desktop']);p.addWidget('org.kde.plasma.marginsseparator');p.addWidget('org.kde.plasma.systemtray');c=p.addWidget('org.kde.plasma.digitalclock');c.currentConfigGroup=['Appearance'];c.writeConfig('showWeekNumbers',!0);p.addWidget('org.kde.plasma.showdesktop')",
          "pro": "panels().map(p=\u003ep.remove());desktops().map(d=\u003e{d.widgets().map(w=\u003ew.remove());let s=desktopById(d.addWidget('org.kde.plasma.systemtray',0,1010,500,70).readConfig('SystrayContainmentId'));s.currentConfigGroup=['General'];s.writeConfig('showAllItems',true);s.writeConfig('iconSpacing',1);c=d.addWidget('org.kde.plasma.digitalclock',1620,1010,300,70);c.writeConfig('UserBackgroundHints','StandardBackground');c.currentConfigGroup=['Appearance'];c.writeConfig('showSeconds',true);c.writeConfig('dateDisplayFormat','BesideTime');c.writeConfig('showWeekNumbers',true)});"
        }
      },
      "neovim": {
        "path": "~/.config/nvim/lua/config/options.lua"
      },
      "obs": {
        "backgrounds": {
          "dark": "Yami",
          "light": "Light"
        },
        "path": "~/.config/obs-studio/global.ini"
      },
      "onlyoffice": {
        "path": "~/.config/onlyoffice/DesktopEditors.conf"
      },
      "tmux": {
        "path": "~/.config/tmux/tmux.conf"
      },
      "vscode": {
        "path": "~/.config/Code/User/settings.json",
        "themes": {
          "dark": {
            "ayu": "Ayu Dark",
            "catppuccin": "Catppuccin Mocha",
            "onedark": "Atom One Dark"
          },
          "light": {
            "ayu": "Ayu Light",
            "catppuccin": "Catppuccin Latte",
            "onedark": "Atom One Light"
          }
        }
      },
      "wezterm": {
        "path": "~/.config/wezterm/wezterm.lua"
      }
    }
  },
  "commands": {
    "background": [
      {
        "name": "neovim",
        "commands": [
          "sed -i \"6s|background = '.*'|background = '%background%'|\" %path%",
          "bash -c \"for i in $(ls ${XDG_RUNTIME_DIR}/nvim.*.0); do nvim --server $i --remote-send ':set background=%background%\u003cCR\u003e'; done\""
        ]
      },
      {
        "name": "wezterm",
        "commands": [
          "sed -i \"s/theme = '.*'/theme = '%theme%-%background%'/\" %path%"
        ]
      },
      {
        "name": "tmux",
        "commands": [
          "sed -i '2s|\\(s/.*-\\).*|\\1%background%.conf|' %path%",
          "tmux source-file %path%"
        ]
      },
      {
        "name": "vscode",
        "commands": [
          "sed -i 's|\"workbench.colorTheme\": .*|\"workbench.colorTheme\": \"%themes.%background%.%theme%%\",|' %path%"
        ]
      },
      {
        "name": "kde",
        "commands": [
          "lookandfeeltool -a %backgrounds.%background%% \u003e/dev/null 2\u003e\u00261"
        ]
      },
      {
        "name": "obs",
        "commands": [
          "sed -i s|CurrentTheme3=.*|CurrentTheme3=%backgrounds.%background%%| %path%"
        ]
      },
      {
        "name": "onlyoffice",
        "commands": [
          "sed -i s|UITheme2=.*|UITheme2=theme-%background%| %path%"
        ]
      }
    ],
    "font": [
      {
        "name": "wezterm",
        "commands": [
          "sed -i \"s|\\(wezterm\\.font('\\).*')|\\1%fonts.%font%%')|\" %path%"
        ]
      },
      {
        "name": "vscode",
        "commands": [
          "sed -i 's|\\(\"editor\\.fontFamily\": \"\\).*|\\1%fonts.%font%%\",|' %path%"
        ]
      }
    ],
    "keyboard": [
      {
        "name": "kde",
        "commands": [
          "setxkbmap -layout %keyboards.%keyboard%%"
        ]
      }
    ],
    "shortcut": [
      {
        "name": "kde",
        "commands": [
          "bash -c '%shortcuts.%shortcut%%'",
          "qdbus org.kde.KWin /KWin reconfigure",
          "killall kglobalaccel5"
        ]
      }
    ],
    "taskbar": [
      {
        "name": "kde",
        "commands": [
          "qdbus org.kde.plasmashell /PlasmaShell org.kde.PlasmaShell.evaluateScript \"%taskbars.%taskbar%%\"",
          "kwriteconfig5 --file 'kwinrc' --group 'Plugins' --key 'slideEnabled' '%slides.%taskbar%%'",
          "qdbus org.kde.KWin /KWin reconfigure"
        ]
      }
    ],
    "theme": [
      {
        "name": "neovim",
        "commands": [
          "sed -i \"9s|'.*'|'%theme%'|\" %path%",
          "bash -c \"for i in $(ls ${XDG_RUNTIME_DIR}/nvim.*.0); do nvim --server $i --remote-send ':colorscheme %theme%\u003cCR\u003e'; done\""
        ]
      },
      {
        "name": "wezterm",
        "commands": [
          "sed -i \"s/theme = '.*'/theme = '%theme%-%background%'/\" %path%"
        ]
      },
      {
        "name": "tmux",
        "commands": [
          "sed -i 2s|s/.*-|s/%theme%-| %path%",
          "tmux source-file %path%"
        ]
      },
      {
        "name": "vscode",
        "commands": [
          "sed -i 's|\"workbench.colorTheme\": .*|\"workbench.colorTheme\": \"%themes.%background%.%theme%%\",|' %path%"
        ]
      },
      {
        "name": "insomnium",
        "commands": [
          "sed -i 's|\"lightTheme\":\"[^\"]*\"|\"lightTheme\":\"%themes.light.%theme%%\"|' %path%",
          "sed -i 's|\"darkTheme\":\"[^\"]*\"|\"darkTheme\":\"%%themes.dark.%theme%%\"|' %path%"
        ]
      }
    ]
  }
}
