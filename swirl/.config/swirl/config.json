{
  "variables": {
    "global": {
      "background": "light",
      "keyboard": "pro",
      "taskbar": "pro",
      "theme": "catppuccin"
    },
    "applications": {
      "alacritty": {
        "path": "~/.config/alacritty/alacritty.yml"
      },
      "google chrome": {
        "background_dark": "1s|#|--|g",
        "background_light": "1s|--|#|g",
        "path": "~/.config/chrome-flags.conf"
      },
      "insomnia": {
        "ayu_dark": "ayu-dark",
        "ayu_light": "ayu-light",
        "catppuccin_dark": "catppuccin-mocha",
        "catppuccin_light": "catppuccin-latte",
        "onedark_dark": "one-dark",
        "onedark_light": "one-light",
        "path": "~/.config/Insomnia/insomnia.Settings.db",
        "tokyonight_dark": "tokyo-night",
        "tokyonight_light": "tokyo-night-light"
      },
      "kde": {
        "background_dark": "org.kde.breezedark.desktop",
        "background_light": "org.kde.breeze.desktop",
        "keyboard_latam": "latam -option",
        "keyboard_pro": "pro -option caps:swapescape",
        "slide_default": "--delete",
        "slide_pro": "false",
        "taskbar_default": "panels().map(p=\u003ep.remove());desktops().map(d=\u003ed.widgets().map(w=\u003ew.remove()));let p=new Panel;p.location='bottom';p.height=2*Math.floor(gridUnit*2.3/2);let m=p.addWidget('org.kde.plasma.kickoff');m.currentConfigGroup=['Shortcuts'];m.writeConfig('global','Alt+F1');p.addWidget('org.kde.plasma.pager');let t=p.addWidget('org.kde.plasma.icontasks');t.currentConfigGroup=['General'];t.writeConfig('launchers',['applications:kitty.desktop','applications:google-chrome.desktop','applications:org.kde.dolphin.desktop']);p.addWidget('org.kde.plasma.marginsseparator');p.addWidget('org.kde.plasma.systemtray');c=p.addWidget('org.kde.plasma.digitalclock');c.currentConfigGroup=['Appearance'];c.writeConfig('showWeekNumbers',!0);p.addWidget('org.kde.plasma.showdesktop')",
        "taskbar_pro": "panels().map(p=\u003ep.remove());desktops().map(d=\u003e{d.widgets().map(w=\u003ew.remove());let s=desktopById(d.addWidget('org.kde.plasma.systemtray',0,1010,500,70).readConfig('SystrayContainmentId'));s.currentConfigGroup=['General'];s.writeConfig('showAllItems',true);s.writeConfig('iconSpacing',1);c=d.addWidget('org.kde.plasma.digitalclock',1620,1010,300,70);c.writeConfig('UserBackgroundHints','StandardBackground');c.currentConfigGroup=['Appearance'];c.writeConfig('showSeconds',true);c.writeConfig('dateDisplayFormat','BesideTime');c.writeConfig('showWeekNumbers',true)});"
      },
      "kitty": {
        "path": "~/.config/kitty/kitty.conf"
      },
      "neovim": {
        "path": "~/.config/nvim/lua/config/options.lua"
      },
      "obs": {
        "background_dark": "Yami",
        "background_light": "Light",
        "path": "~/.config/obs-studio/global.ini"
      },
      "onlyoffice": {
        "path": "~/.config/onlyoffice/DesktopEditors.conf"
      },
      "tmux": {
        "path": "~/.config/tmux/tmux.conf"
      },
      "vscode": {
        "ayu_dark": "Ayu Dark",
        "ayu_light": "Ayu Light",
        "catppuccin_dark": "Catppuccin Mocha",
        "catppuccin_light": "Catppuccin Latte",
        "onedark_dark": "Atom One Dark",
        "onedark_light": "Atom One Light",
        "path": "~/.config/Code/User/settings.json",
        "tokyonight_dark": "Tokyo Night",
        "tokyonight_light": "Tokyo Night Light"
      }
    }
  },
  "commands": {
    "background": [
      {
        "name": "neovim",
        "commands": [
          "sed -i \"6s|background = '.*'|background = '%background%'|\" %path%",
          "nvim --server /tmp/nvim.socket --remote-send \":set background=%background%\u003cCR\u003e\""
        ]
      },
      {
        "name": "kitty",
        "commands": [
          "sed -i \"6s|-.*\\.|-%background%.|\" %path%",
          "bash -c 'for i in $(pgrep kitty); do kill -SIGUSR1 $i; done'"
        ]
      },
      {
        "name": "alacritty",
        "commands": [
          "sed -i '17s|\\(s/.*-\\).*|\\1%background%.yml|' %path%"
        ]
      },
      {
        "name": "tmux",
        "commands": [
          "sed -i '2s|\\(s/.*-\\).*|\\1%background%.conf|' %path%",
          "tmux source-file %path%"
        ]
      },
      {
        "name": "vscode",
        "commands": [
          "sed -i 's|\"workbench.colorTheme\": .*|\"workbench.colorTheme\": \"%%theme%_%background%%\",|' %path%"
        ]
      },
      {
        "name": "kde",
        "commands": [
          "lookandfeeltool -a %background_%background%% \u003e/dev/null 2\u003e\u00261"
        ]
      },
      {
        "name": "google chrome",
        "commands": [
          "sed -i '%background_%background%%' %path%"
        ]
      },
      {
        "name": "obs",
        "commands": [
          "sed -i s|CurrentTheme3=.*|CurrentTheme3=%background_%background%%| %path%"
        ]
      },
      {
        "name": "onlyoffice",
        "commands": [
          "sed -i s|UITheme2=.*|UITheme2=theme-%background%| %path%"
        ]
      }
    ],
    "keyboard": [
      {
        "name": "kde",
        "commands": [
          "setxkbmap -layout %keyboard_%keyboard%%"
        ]
      }
    ],
    "taskbar": [
      {
        "name": "kde",
        "commands": [
          "qdbus org.kde.plasmashell /PlasmaShell org.kde.PlasmaShell.evaluateScript \"%taskbar_%taskbar%%\"",
          "kwriteconfig5 --file 'kwinrc' --group 'Plugins' --key 'slideEnabled' '%slide_%taskbar%%'",
          "qdbus org.kde.KWin /KWin reconfigure"
        ]
      }
    ],
    "theme": [
      {
        "name": "neovim",
        "commands": [
          "sed -i \"9s|'.*'|'%theme%'|\" %path%",
          "nvim --server /tmp/nvim.socket --remote-send \":colorscheme %theme%\u003cCR\u003e\""
        ]
      },
      {
        "name": "kitty",
        "commands": [
          "sed -i 6s|s/.*-|s/%theme%-| %path%",
          "bash -c 'for i in $(pgrep kitty); do kill -SIGUSR1 $i; done'"
        ]
      },
      {
        "name": "alacritty",
        "commands": [
          "sed -i 17s|s/.*-|s/%theme%-| %path%"
        ]
      },
      {
        "name": "tmux",
        "commands": [
          "sed -i 2s|s/.*-|s/%theme%-| %path%",
          "tmux source-file %path%"
        ]
      },
      {
        "name": "vscode",
        "commands": [
          "sed -i 's|\"workbench.colorTheme\": .*|\"workbench.colorTheme\": \"%%theme%_%background%%\",|' %path%"
        ]
      },
      {
        "name": "insomnia",
        "commands": [
          "sed -i 's|\"lightTheme\":\"[^\"]*\"|\"lightTheme\":\"%%theme%_light%\"|' %path%",
          "sed -i 's|\"darkTheme\":\"[^\"]*\"|\"darkTheme\":\"%%theme%_dark%\"|' %path%"
        ]
      }
    ]
  }
}